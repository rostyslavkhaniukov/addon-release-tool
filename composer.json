{
    "name": "rostyslavkhaniukov/github-client",
    "description": "AirSlate client library.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "AirSlate\\Releaser\\": "src"
        }
    },
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "6.2.*|6.3.*",
        "illuminate/support": "^5.8",
        "rostyslavkhaniukov/fluffy-github-client": "dev-master",
        "vlucas/phpdotenv": "^3.4"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.3",
        "phpunit/phpunit": "^7.3"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:rostyslavkhaniukov/fluffy-github-client.git"
        }
    ]
}
